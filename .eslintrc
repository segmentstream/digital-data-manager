{
  "extends": "airbnb-base",
  "rules": {
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "import/first": 0,
    "import/prefer-default-export": 0,
    "no-multi-assign": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "no-useless-escape": 0,
    "max-len": ["error", {"code": 120, "comments": 100}]
  },
  "env": {
    "browser": true
  }
}
